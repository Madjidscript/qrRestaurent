<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/stylesheets/menu.css">
    <title>Document</title>
</head>
<body>
    <%- include('partials/hearder') %>
   
    <form action="/admin/stock" method="post">
        <h2>Formulaire de stocks</h2>
        <% if ( typeof vv !== "undefined" && vv ) { %>
            <p class="message"><%= vv %></p>
            <% } %>
         
         
        <label for="plat">Choisissez votre plat :</label>
        <select id="plat" name="id_Souscat">
            <% if( typeof recups !== "undefined" && recups ) { %>
            <% recups.forEach(function(element) { %>
            <option value="<%= element._id %>"> <%= element.id_cath.nom %> <span><%= element.nom %></span></option>
            <% }); %>
            <% } %>
        </select>
        <input type="number" placeholder="Entrer le nombre de stock" name="nombre" id="" required>
       
        <button class="valider" type="submit">Commander</button>
    </form>
    <!-- <script src="/javascripts/form.js"></script> -->
    <%- include('partials/loader') %>
</body>
</html>